<!--pages/home/home.wxml-->
<!-- 1.小程序的数据绑定: {{}} -->
<view>Hello {{name}}</view>
<view>年龄：{{age}}</view>

<!-- 2.列表展示: wx:for -->
<view wx:for="{{EDG}}">{{item.ID}}-{{item.age}}</view>

<!-- 3.事件监听改变data -->
<view>当前计数：{{counter}}</view>
<button size="mini" bindtap='increment'>+</button>
<button size="mini" bindtap='decrement'>-</button>